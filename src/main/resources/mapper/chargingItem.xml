<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.Property.Mapper.ChargingItemMapper">
	<resultMap id="chargingItem" type="com.Property.Entity.ChargingItem">
		<id column="item_id" property="item_id" jdbcType="CHAR"/>
		<result column="item_title" property="item_title" jdbcType="VARCHAR"/>
	</resultMap>

	<sql id="BaseColumnList">
		item_id,item_title
	</sql>

	<select id="getAll" resultMap="chargingItem">
		SELECT
		<include refid="BaseColumnList"/>
		FROM chargingItem
	</select>
</mapper>